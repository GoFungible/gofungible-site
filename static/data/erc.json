	{ "erc": "EIP-2612", "keywords": "standardization.permit", "title": "Permit Extension for EIP-20 Signed Approvals", "desc": "EIP-20 approvals via EIP-712 secp256k1 signatures", "url": "https://eips.ethereum.org/EIPS/eip-2612" },

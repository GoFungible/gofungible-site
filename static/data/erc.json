	{ "erc": "EIP-7837", "keywords": "standardization.supply.mint", "title": "Diffusive Tokens", "desc": "A fungible token that mints new tokens on transfer, charges a per-token native fee, and enforces a capped supply.", "url": "https://eips.ethereum.org/EIPS/eip-7837" },
	{ "erc": "EIP-7770", "keywords": "standardization.supply.mint", "title": "Fractional Reserve Token", "desc": "An interface for tokens that maintain a peg to an underlying token while being only partially redeemable to this token.", "url": "https://eips.ethereum.org/EIPS/eip-7770" },
	{ "erc": "EIP-7538", "keywords": "standardization.supply.mint", "title": "Multiplicative Tokens", "desc": "Incorporates a multiplier field to ERC-20 and ERC-1155 for fractional token values", "url": "https://eips.ethereum.org/EIPS/eip-7538" },
	{ "erc": "EIP-7425", "keywords": "standardization.supply.mint", "title": "Tokenized Reserve", "desc": "Transparent reserve fund on-chain with stakeholder participation.", "url": "https://eips.ethereum.org/EIPS/eip-7425" },
	{ "erc": "EIP-5615", "keywords": "standardization.supply.mint", "title": "Supply Extension", "desc": "A simple mechanism to fetch token supply data from ERC-1155 tokens", "url": "https://eips.ethereum.org/EIPS/eip-5615" },
	{ "erc": "EIP-5679", "keywords": "standardization.supply.mint", "title": "Token Minting and Burning", "desc": "An extension for minting and burning EIP-20, EIP-721, and EIP-1155 tokens", "url": "https://eips.ethereum.org/EIPS/eip-5679" },
	{ "erc": "EIP-918",  "keywords": "standardization.supply.mint", "title": "Mineable Token Standard", "desc": "A specification for a standardized Mineable Token that uses a Proof of Work algorithm for distribution.", "url": "https://eips.ethereum.org/EIPS/eip-918" },
	{ "erc": "EIP-6604", "keywords": "standardization.supply.mint", "title": "Abstract Token", "desc": "move tokens on- and off-chain as desired, enabling zero-cost minting while preserving on-chain composability", "url": "https://eips.ethereum.org/EIPS/eip-6604" },

	{ "erc": "EIP-621",  "keywords": "standardization.supply.burn", "title": "Minting and burning", "desc": "ERC-621 standardizes minting and burning functions for ERC-20 tokens, improving interoperability.", "url": "https://onekey.so/blog/ecosystem/understanding-erc-621-the-standard-for-minting-and-burning-tokens/" },
	{ "erc": "EIP-7818", "keywords": "standardization.supply.burn", "title": "Expirable ERC-20", "desc": "An ERC-20 extension for creating fungible tokens with expiration, supporting time-limited use cases.", "url": "https://eips.ethereum.org/EIPS/eip-7818" },
